    <selector xmlns:android="http://schemas.android.com/apk/res/android">
        <!-- 背景选择器 -->
        <!-- 当我鼠标按下的时候 -->
        <item android:state_pressed="true" android:drawable="@drawable/tabbar_icon_lis_n"/>
